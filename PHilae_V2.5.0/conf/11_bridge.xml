<server name="philae" class="org.jpos.q2.iso.QServer" logger="logger">
    <attr name="port" type="java.lang.Integer">5050</attr>
    <channel name="philae.channel" class="org.jpos.iso.channel.QuipuChannel" 
                 packager="org.jpos.iso.packager.GenericPackager" logger="logger">
        <property name="packager-config" value="pack/iso87ascii.xml"/>
        <property name="timeout" value="1800000"/>
    </channel>
    <request-listener class="PHilae.TXNListener" logger="logger"/>
</server>