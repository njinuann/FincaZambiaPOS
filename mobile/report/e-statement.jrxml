	<parameter name="AcctNoParam" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT sAccountNo AcctNo, to_date(dtEndDt,'dd/mm/yyy') StatementDt, sName1 Name, sCurrency Currency, sAcctType AcctType, (sAddressLine1 ||' ' ||sAddressLine2) Address,
NVL(((select NVL(nCurBal,0) from dp_stmt where sAccountNo = $P{AcctNoParam} and nCount = (select max(nCount) from dp_stmt where sAccountNo = $P{AcctNoParam})) + (select NVL(sum(nAmt),0) from dp_stmt where sAccountNo = $P{AcctNoParam} and nDebitCredit > 0) - (select NVL(sum(nAmt),0) from dp_stmt where sAccountNo = $P{AcctNoParam} and nDebitCredit = 0)),0) OpenBal,
NVL((select NVL(sum(nAmt),0) from dp_stmt where sAccountNo = $P{AcctNoParam} and nDebitCredit = 0),0) TotalCR,
NVL((select NVL(sum(nAmt),0) from dp_stmt where sAccountNo = $P{AcctNoParam} and nDebitCredit > 0),0) TotalDR,
NVL((select NVL(nCurBal,0) from dp_stmt where sAccountNo = $P{AcctNoParam} and nCount = (select max(nCount) from dp_stmt where sAccountNo = $P{AcctNoParam})),0) CloseBal,
to_date(dtEffectiveDt,'dd/mm/yyy') TranDate, sDescription TranDetails, NVL(to_char(nCheckNo), '') Reference,
nAmt Amount, nvl(nCurBal,0) CurBal, nDebitCredit DRCR
FROM   dp_stmt where sAccountNo = $P{AcctNoParam} ORDER BY nCount ASC]]>
